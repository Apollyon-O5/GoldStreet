@model IEnumerable<GoldStreet.Transacciones>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cuentas.CuentaID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Servicios.NombreServicio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cuentas.CuentaID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Servicios.NombreServicio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TransaccionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TransaccionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TransaccionID })
        </td>
    </tr>
}

</table>
